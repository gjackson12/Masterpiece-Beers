<%= simple_form_for @beer_label, :html => {:multipart => true} do |f| %>
  <%= f.input :beer_name, label: "Beer Name" %>
  <%= f.input :brewery %>
  <%= f.input :origin %>
  <%= f.input :description %>
  <%= f.input :tag %>
  <%= f.file_field :image %><br/ >
  <%= f.button :submit, "Add Beer Label" %>
<%- end -%>


<%= link_to 'Back Home', beer_labels_path %>